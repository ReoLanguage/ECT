<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="org.ect.codegen.v2.proxy.cli.Application">
         </run>
      </application>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="cwi.reo.proxygen.cliapp"
            name="Reo Proxy Generator">
         <property
               name="appName"
               value="Reo Proxy Generator">
         </property>
      </product>
   </extension>

</plugin>
